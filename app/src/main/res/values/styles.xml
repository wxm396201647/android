    <!-- 带删除按钮的文本输入框 -->
    <style name="clear_editText_userName">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/dp_16</item>
        <item name="android:paddingRight">@dimen/dp_22</item>
        <item name="android:drawableRight">@mipmap/emotionstore_progresscancelbtn</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@null</item>
    </style>
    <!-- 带删除按钮的文本输入框 -->
    <style name="introduce_editText_userName">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/dp_16</item>
        <item name="android:paddingRight">@dimen/dp_22</item>
        <item name="android:gravity">center_vertical</item>
